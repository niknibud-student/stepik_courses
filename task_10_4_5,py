n = int(input())

cities = {}
for _ in range(n):
    country, *towns = input().split()
    for city in towns:
        cities[city] = country

x = int(input())

for _ in range(x):
    print(cities[input()])


d = {}
for _ in range(int(input())):
    country, *cities = input().split()
    d.update(dict.fromkeys(cities, country))
for _ in range(int(input())):
    print(d[input()])
